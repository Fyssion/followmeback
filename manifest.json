{
	"manifest_version": 3,
	"name": "Follow Me Back",
	"description": "See who doesn't follow you back on Instagram.",
	"version": "0.2.1",
	"license": "MPL-2.0",
	"icons": {
		"64": "icons/icon.png"
	},
	"action": {
		"default_icon": {
			"64": "icons/icon.png"
		},
		"default_popup": "action/index.html",
		"default_title": "Follow Me Back"
	},
	"permissions": [
    	"activeTab",
		"storage",
		"scripting"
  	],
	"host_permissions": [
        "https://*.instagram.com/*"
    ],
	"browser_specific_settings": {
		"gecko": {
			"id": "{13633b5f-c023-4423-9a88-f1fb875d8c74}"
		}
	}
}